#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node@20.7.0/node_modules/node/bin/node_modules:/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node@20.7.0/node_modules/node/node_modules:/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node@20.7.0/node_modules:/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node@20.7.0/node_modules/node/bin/node_modules:/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node@20.7.0/node_modules/node/node_modules:/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node@20.7.0/node_modules:/home/lava/Desktop/SpaceGame/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../node/bin/node"   "$@"
exit $?
